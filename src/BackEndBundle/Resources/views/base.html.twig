    <title>uBlog | {% block title %}{% endblock %}</title>

    {% block stylesheets %}
      <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" type="text/css" rel="stylesheet">
      <link href="{{ asset('backEnd/css/bo_style.css') }}" type="text/css" rel="stylesheet">
